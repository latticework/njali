<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>Jali</id>
    <version>$version$</version>
    <title>Jali</title>
    <description>Jali is a cross platform execution context of explicitly provided crosscutting concers. NJali.Jali is the .NET implementation.</description>
    <authors>Latticework</authors>
    <language>en-US</language>
    <projectUrl>http://www.github.com/latticework/Jali</projectUrl>
    <!--<iconUrl>http://www.newtonsoft.com/content/images/nugeticon.png</iconUrl>-->
    <licenseUrl>https://raw.githubusercontent.com/latticework/njali/master/LICENSE</licenseUrl>
    <tags>logging configuration metrics security</tags>
    <dependencies>
      <group targetFramework="net45">
        <dependency id="Jali.Core" version="$version$" />
      </group>
      <group targetFramework="wp8">
        <dependency id="Jali.Core" version="$version$" />
      </group>
      <group targetFramework="win8">
        <dependency id="Jali.Core" version="$version$" />
      </group>
      <group targetFramework="wpa81">
        <dependency id="Jali.Core" version="$version$" />
      </group>
      <group targetFramework="xamarin.ios">
        <dependency id="Jali.Core" version="$version$" />
      </group>
      <group targetFramework="monotouch">
        <dependency id="Jali.Core" version="$version$" />
      </group>
      <group targetFramework="monoandroid">
        <dependency id="Jali.Core" version="$version$" />
      </group>
      <group targetFramework=".NETPlatform5.0">
        <dependency id="Jali.Core" version="$version$" />
      </group>
    </dependencies>
  </metadata>
</package>